.address-list{min-height:100vh;padding-top:1px;background-color:#f6f6f6;box-sizing:border-box}.address-list .list-group{margin:10px 0}.address-list .list-group__item{position:relative}.address-list .list-group__item .title{font-size:14px;font-weight:400;line-height:20px;padding-right:48px;word-wrap:break-word}.address-list .list-group__item .title__line1{margin-bottom:6px;color:#4a4a4a}.address-list .list-group__item .title__line2{margin-bottom:10px;color:#4a4a4a}.address-list .list-group__item .title__line3{margin-bottom:5px;color:#9b9b9b}.address-list .list-group__item .action{position:absolute;top:10px;right:15px}.address-list .list-group__item .action text{height:17px;font-size:12px;font-weight:400;color:#9b9b9b;line-height:17px;margin-left:10px}.address-list .list-group__item .action .icon{position:relative;top:2px}.address-list .list-group__item .defalut-tag{position:absolute;top:40px;right:15px;color:#fff;width:32px;height:16px;text-align:center;line-height:16px;background:#f5c8c2;font-size:11px;font-weight:400}