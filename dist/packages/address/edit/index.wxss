.address-edit{min-height:100vh;padding-top:10px;background-color:#f6f6f6;box-sizing:border-box}.default-btn{position:relative;width:40px;height:23px;border-radius:16px;border:1px solid rgba(0,0,0,.1)}.default-btn--active{border-color:rgba(227,172,180,.1);background-color:#f5c8c2}.default-btn__slider{position:absolute;top:0;width:21px;height:21px;background:#fff;box-shadow:0 1px 1px 0 rgba(0,0,0,.05),0 1px 1px 0 rgba(0,0,0,.1),0 2px 1px 0 rgba(0,0,0,.05);border:1px solid rgba(0,0,0,.1);border-radius:40px;background-color:#fff}.default-btn__slider--left{left:0}.default-btn__slider--right{left:18px;border-color:inherit}.edit-btn{margin:90rpx auto;width:600rpx;height:92rpx;color:#fff;font-size:18px;font-weight:500;border-radius:8rpx;text-align:center;line-height:92rpx;background:#f5c8c2;box-shadow:1px 1px 6px 3px rgba(96,66,70,.08)}