.wallet{padding-bottom:370rpx}.coupon-wrapper{width:100%;background:#fff;padding:5px 15px;box-sizing:border-box}.coupon-wrapper .coupon{height:112px;display:-webkit-box;display:flex;padding-left:22px;border-radius:4px;box-shadow:1px 1px 6px 3px rgba(96,66,70,.08)}.coupon-wrapper .coupon__left{-webkit-box-flex:1;flex:1}.coupon-wrapper .coupon__left .title{margin-top:28px;margin-bottom:12px;color:#4a4a4a;height:20px;font-size:14px;font-weight:500;line-height:20px;font-family:PingFangSC-Medium}.coupon-wrapper .coupon__left .time-scope{height:14px;font-size:10px;font-family:PingFangSC-Light;font-weight:300;color:#4a4a4a;line-height:14px}.coupon-wrapper .coupon__right{width:123px;position:relative;border-radius:0 4px 4px 0;background-image:radial-gradient(circle at 0 8px,transparent 4px,#f5c8c2 6px,#f5c8c2 0);background-size:200px 16px;background-position:0 0,200px 0;background-repeat-x:no-repeat}.coupon-wrapper .coupon__right .value{position:absolute;top:50%;width:100%;height:50px;line-height:50px;text-align:center;-webkit-transform:translateY(-50%);transform:translateY(-50%)}.coupon-wrapper .coupon__right .value__unit-front{color:#fff;font-size:18px;margin-right:5px}.coupon-wrapper .coupon__right .value__content{color:#fff;font-size:35px;font-weight:600}.coupon-wrapper .coupon__right .value__unit-end{color:#fff;font-size:18px;margin-left:3px}.fetch-coupon{position:fixed;left:0;bottom:0;width:100%;box-sizing:border-box;background-color:#fff}.fetch-coupon__field{border-radius:6px;margin-bottom:24px;box-shadow:0 1px 6px 0 #e1e2e6}.fetch-coupon__btn{color:#fff;font-size:18px;font-weight:500;text-align:center;height:56px;line-height:56px;border-radius:6px;background:#f5c8c2;font-family:PingFangSC-Medium;box-shadow:1px 1px 6px 3px rgba(96,66,70,.08)}